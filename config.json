{
	"version": "0.0.5",
	"repository": {
		"local_branch": "master",
		"repository_path": "../phpunit-cluster-tests",
		"server_connection_timeout": 15000,
		"client_connection_timeout": 15000
	},
	"task_balancer": {
		"failed_attempts": 2
	},
	"server_socket": {
		"port": 8099
	},
	"stats_socket": {
		"port": 8101
	},
	"udp_socket": {
		"port": 8095
	},
	"web": {
		"port": 8097
	},
	"phpunit_runner": {
		"cmd": "BAMBOO_UNIT_TEST_DATABASE=b2bcenter_cluster_test php /opt/phpunit/phpunit-4.8.15.phar",
		"cmd_suffix": "--bootstrap /var/reps/b2bcenter/www_b2b/includes/tests/bootstrap.php -c /var/reps/b2bcenter/www_b2b/includes/tests/configuration.xml",
		"result_json_file": "result.json"
	},
	"parser" : {
		"baseDirs" : [
			"example"
		]
	},
	"db": {
		"cmd_update": "php /var/reps/b2bcenter/automation/bamboo/update_db_all.php"
	},
	"fs": {
		"cmd_update": "php /var/reps/b2bcenter/www_b2b/admin/cli/make_files_root_dirs.php"
	},
	"statistic": {
		"exec_time_log": "./weight.json"
	}
}
